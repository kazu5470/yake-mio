<div class="container">
      <h2>新規会員登録</h2>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= render "public/shared/error_messages", resource: resource %>
          <table>
            <tr>
              <div class="field">
              <tr>
                氏名 (姓)<%= f.text_field :first_name %>(名)<%= f.text_field :last_name %>
              </tr>
              </div>
            </tr>
            <tr></tr>
            <tr>
              <div class="field">
                <tr>フリガナ (セイ)<%= f.text_field :first_name_kana %>(メイ)<%= f.text_field :last_name_kana %></tr>
              </div>  
            </tr>  
            <tr>
              <div class="field">
                <%= f.label :年齢 %>
                <%= f.select :age, [[15, 15], [16, 16], [17, 17], [18, 18], [19, 19], [20, 20], [21, 21],[22, 22], [23, 23], [24, 24], [25, 25], [26, 26], [27,27], [28,28], [29,29], [30,30], [31, 31], [32,32], [33,33], [34,34], [35, 35], [36,36], [37,37], [38,38], [39,39], [40,40], [41,41], [42,42], [43,43], [44,44], [45,45], [46,46,], [47,47],[48,48],[49,49],[50,50],[51,51],[52,52],[53,53],[54,54],[55,55],[56,56],[57,57],[58,58],[59,59],[60,60],[61,61],[62,62],[63,63],[64,64],[65,65],[66,66],[67,67],[68,68],[69,69],[70,70],[71,71],[72,72],[73,73],[74,74],[75,75],[76,76],[77,77],[78,78],[79, 79], [80, 80], [81, 81], [82, 82], [83, 83], [84, 84], [85, 85], [86, 86], [87, 87], [88, 88], [89, 89], [90, 90]] %>
              </div>
              <!--<div class="field">-->
              <!--  <tr>年齢<#%= f.text_field :age %></tr>-->
              <!--</div>-->
            </tr>
                <div class="field">
                  <tr><%= f.label :メールアドレス %>
                  <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
                  </tr>
                </div>
            <tr>
                <div class="field">
                  <%= f.label :パスワード %>
                  <% if @minimum_password_length %>
                  <em>(<%= @minimum_password_length %> 桁以上)</em>
                  <% end %>
                  <%= f.password_field :password, autocomplete: "new-password" %>
                </div>
            </tr>  
            <tr>
                <div class="field">
                  <%= f.label :パスワード確認 %>
                  <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
                </div>
            </tr>  
                <div class="actions">
                  <%= f.submit "Sign up" %>
                </div>
          </table>      
        <% end %>
  </div>  
</div>
<%= render "public/shared/links" %>
